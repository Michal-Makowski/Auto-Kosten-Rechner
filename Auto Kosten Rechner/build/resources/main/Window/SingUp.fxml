<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="275.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Window.SingUpControler">
    <children>
        <Button fx:id="buttonSingUp" layoutX="164.0" layoutY="185.0" mnemonicParsing="false" onAction="#buttonSingUpClicked" prefHeight="39.0" prefWidth="199.0" text="Sing Up">
            <font>
                <Font size="18.0" />
            </font></Button>
        <TextField fx:id="textFieldNewUsername" layoutX="164.0" layoutY="39.0" prefHeight="25.0" prefWidth="199.0" />
        <TextField fx:id="textFieldNewPassword" layoutX="164.0" layoutY="92.0" prefHeight="25.0" prefWidth="199.0" />
        <Label layoutX="23.0" layoutY="40.0" text="New Username:">
            <font>
                <Font name="Calibri Bold" size="18.0" />
            </font></Label>
        <Label layoutX="26.0" layoutY="94.0" text="Password:">
            <font>
                <Font name="Calibri Bold" size="18.0" />
            </font></Label>
        <Button fx:id="buttonLogIn" layoutX="313.0" layoutY="233.0" mnemonicParsing="false" onAction="#buttonLogInClicked" style="-fx-background-color: transparent; -fx-cursor: hand;" text="Login" textFill="#2508e8" underline="true">
            <font>
                <Font name="Calibri Bold" size="14.0" />
            </font>
        </Button>
        <Label layoutX="164.0" layoutY="238.0" text="Haben sie schon konto?">
            <font>
                <Font name="Calibri Bold" size="14.0" />
            </font>
        </Label>
      <TextField fx:id="textFieldNewPasswordRepeat" layoutX="164.0" layoutY="138.0" prefHeight="25.0" prefWidth="199.0" />
      <Label layoutX="26.0" layoutY="142.0" prefHeight="22.0" prefWidth="121.0" text="Password Wiederholen">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="labelError" layoutX="164.0" layoutY="164.0" prefHeight="17.0" prefWidth="199.0" textFill="RED">
         <font>
            <Font name="Calibri" size="12.0" />
         </font>
      </Label>
    </children>

</AnchorPane>
