<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="275.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogInControler">
   <children>
      <Button fx:id="buttonLogin" layoutX="164.0" layoutY="156.0" mnemonicParsing="false" onAction="#buttonLoginClicked" prefHeight="39.0" prefWidth="199.0" text="Login">
         <font>
            <Font size="18.0" />
         </font></Button>
      <TextField fx:id="textFieldUsername" layoutX="164.0" layoutY="38.0" prefHeight="25.0" prefWidth="199.0" />
      <TextField fx:id="textFieldPassword" layoutX="164.0" layoutY="92.0" prefHeight="25.0" prefWidth="199.0" />
      <Label layoutX="62.0" layoutY="39.0" text="Username:">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font></Label>
      <Label layoutX="67.0" layoutY="95.0" text="Password:">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font></Label>
      <Button fx:id="buttonSingUp" layoutX="264.0" layoutY="211.0" mnemonicParsing="false" onAction="#buttonSingUpClicked" style="-fx-background-color: transparent; -fx-cursor: hand;" text="Sing Up" textFill="#2508e8" underline="true">
         <font>
            <Font name="Calibri Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="164.0" layoutY="216.0" text="Kein konto?">
         <font>
            <Font name="Calibri Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="labelError" layoutX="165.0" layoutY="129.0" prefHeight="17.0" prefWidth="199.0" textFill="RED">
         <font>
            <Font name="Calibri" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
